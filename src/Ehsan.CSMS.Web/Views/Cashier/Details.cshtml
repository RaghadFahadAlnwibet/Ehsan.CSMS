@model CashierResponse
@{
    ViewData["Title"] = "Cashier Details";
}


<form method="post" asp-action="Edit" asp-controller="Cashier" enctype="multipart/form-data" class="p-4 shadow-sm border rounded mb-5">
    <h2 class="mb-4 text-darl">Details</h2>

    <div class="mb-3">
        @Html.LabelFor(c => c.Id, new { @class = "form-label" })
        @Html.TextBoxFor(c => c.Id, new { @class = "form-control", @readonly = "readonly" })
    </div>

    <div class="mb-3">
        @Html.LabelFor(c => c.Name, new { @class = "form-label" })
        @Html.TextBoxFor(c => c.Name, new { @class = "form-control", @readonly = "readonly" })
    </div>
</form>
